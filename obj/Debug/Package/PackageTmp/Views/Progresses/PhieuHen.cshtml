@model Int32
@using NiceHandles.Models
@{
    var db = new NHModel();
    var progress = db.Progresses.FirstOrDefault(x => x.type == (int)XProgress.eType.GhiChu);
    var hoso_id = db.di1cua.Where(x => x.id == Model).Select(x => x.hoso_id).Single();
    var files = db.progress_file.Where(x => x.hoso_id == hoso_id && x.progress_id == progress.id && x.type == (int)XProgressFile.eType.PhieuTiepNhan);
}
<section id="ss@("_" + progress.type + Model)">
    <h4>PHIẾU TIẾP NHẬN VÀ HẸN NGÀY TRẢ KẾT QUẢ</h4>
    <div class="row">
        <div class="form-group">
            <div class="col-md-2" style="text-align: right;">
                Phiếu hẹn
            </div>
            <div class="col-md-4">
                <input type="file" class="btn btn-info txtPhieuHen form-control" />
            </div>
            <div class="col-md-2">
                <input value="Upload" type="button" onclick="UploadProgressFile('#ss@("_" + progress.type + Model) .txtPhieuHen','#ss@("_" + progress.type + Model) .lstPhieuHen', @(hoso_id) , @(progress.id) , @((int)XProgressFile.eType.PhieuTiepNhan) );" class="btn btn-info" />
            </div>
            <div class="col-md-4 lstPhieuHen" style="padding-right: 0; padding-left: 0;">
                <ul style="margin:0; padding: 0; list-style: decimal;">
                    @foreach (var item in files)
                    {
                        <li>
                            <a href="@item.url" target="_blank">@item.name</a>
                        </li>
                    }
                </ul>
            </div>
        </div>
    </div>
</section>