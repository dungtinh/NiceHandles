@model IEnumerable<NiceHandles.Models.Xdidiachinh>
@using NiceHandles.Models
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Hợp đồng
        </th>
        <th>
            Loại dịch vụ
        </th>
        <th>
            Nơi nộp
        </th>
        <th>
            Ngày nộp
        </th>
        <th>
            Xã/VPĐK
        </th>
        <th>
            Số HĐ ủy quyền
        </th>
        <th>
            Người được UQ
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.customer.name
            </td>
            <td>
                @item.service.name
            </td>
            <td>
                @item.address.name
            </td>
            <td>
                @item.obj.ngaynop
            </td>
            <td>
                @Xdidiachinh.sIsXa[item.obj.is_xa]
            </td>
            <td>
                @Html.ActionLink("Sửa", "Edit", new { id = item.obj.id }) |
                @Html.ActionLink("Xóa", "Delete", new { id = item.obj.id })
            </td>
        </tr>
    }

</table>
